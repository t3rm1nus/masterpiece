{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "outputDirectory": "dist/client",
  "ignoreFiles": [
    "public/imagenes/*",
    "!public/imagenes/og/**",
    "!public/imagenes/notfound/**"
  ]
}
